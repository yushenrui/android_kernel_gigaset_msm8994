	gpio@c200 { /* GPIO 3 */
		qcom,mode = <0>;
		qcom,pull = <0>;
		qcom,vin-sel = <2>;
		qcom,src-sel = <0>;
		status = "okay";
	};

	gpio@c300 { /* GPIO 4 */
		qcom,mode = <0>;
		qcom,pull = <0>;
		qcom,vin-sel = <2>;
		qcom,src-sel = <0>;
		status = "okay";
	};

	gpio@c400 { /* GPIO 5 */
		qcom,mode = <0>;
		qcom,pull = <0>;
		qcom,vin-sel = <2>;
		qcom,src-sel = <0>;
		status = "okay";
	};
//#ifdef GIGASET_EDIT
//jowen.li@swdp.system, 2015/02/13 added for oem panel

	gpio@c600 { /* GPIO 7 */ 
		/* bl pwm3 request */ 
		qcom,mode = <1>; /* QPNP_PIN_MODE_DIG_OU */ 
		qcom,pull = <5>; /* QPNP_PIN_PULL_NO */ 
		qcom,vin-sel = <2>; /* QPNP_PIN_VIN2 */ 
		qcom,src-sel = <2>; /* QPNP_PIN_SEL_FUNC_1 */ 
		qcom,master-en = <1>; 
		status = "okay"; 
	};

//#else
//	gpio@c600 { /* GPIO 7 */
//		/* NFC pwr request */
//		qcom,mode = <0>;                /* QPNP_PIN_MODE_DIG_IN */
//		qcom,pull = <5>;                /* QPNP_PIN_PULL_NO */
//		qcom,vin-sel = <2>;             /* QPNP_PIN_VIN2 */
